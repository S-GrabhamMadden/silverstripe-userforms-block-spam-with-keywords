{
    "name": "sunnysideup/userforms-block-spam-with-keywords",
    "description": "Checks for keywords (e.g. eric@spam.com.nz) and block submissions of the strings are present",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "Userforms",
        "user defined forms",
        "spam",
        "keywords",
        "eric the spammer",
        "validation"
    ],
    "license": "BSD-3-Clause",
    "require": {
        "silverstripe/userforms": "^6.0"
    },
    "require-dev": {
        "sunnysideup/sswebpack_engine_only": "dev-master",
        "silverleague/ideannotator": "*",
        "phpunit/phpunit": "^9.6",
        "squizlabs/php_codesniffer": "^3.7",
        "silverstripe/standards": "^1",
        "phpstan/extension-installer": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "Sunnysideup\\UserformsBlockSpamWithKeywords\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Nicolaas Thiemen Francken",
            "email": "modules@sunnysideup.co.nz",
            "role": "lead developer"
        }
    ]
}